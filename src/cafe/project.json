{
  "version": "0.1.0-*",
  "buildOptions": {
    "debugType": "portable",
    "warningsAsErrors": true,   
    "emitEntryPoint": true,
    "copyToOutput": { 
      "includeFiles": [ "nlog-client.config", "nlog-server.config", "server.json", "appsettings.json", "cafe.nsi" ]
    }
  },
  "publishOptions": {
    "include": [
        "wwwroot",
        "appsettings.json",
        "nlog-client.config",
        "nlog-server.config",
        "server.json",
        "client.json"
    ]
  },
  "dependencies": {
    "System.Linq": "4.3.0",
    "System.Net.Http": "4.3.0",
    "System.Threading.Tasks": "4.3.0",
    "Microsoft.AspNetCore.Mvc": "1.1.0",
    "Microsoft.AspNetCore.Routing": "1.1.0",
    "Microsoft.AspNetCore.Diagnostics": "1.1.0",
    "Microsoft.AspNetCore.Server.IISIntegration": "1.1.0",
    "Microsoft.AspNetCore.Server.Kestrel": "1.1.0",
    "Microsoft.Extensions.Configuration.EnvironmentVariables": "1.1.0",
    "Microsoft.Extensions.Configuration.FileExtensions": "1.1.0",
    "Microsoft.Extensions.Configuration.Json": "1.1.0",
    "Microsoft.Extensions.Configuration.CommandLine": "1.1.0",
    "NodaTime": "2.0.0-alpha20160729",
    "Microsoft.Extensions.Options.ConfigurationExtensions": "1.1.0",
    "StructureMap.Microsoft.DependencyInjection": "1.0.1",
    "RestEase": "1.3.1",
    "NLog": "5.0.0-beta04",
    "NLog.Extensions.Logging": "1.0.0-rc2-final-2016-05-21",
    "Microsoft.Extensions.Logging": "1.1.0",
    "Microsoft.Extensions.Logging.Console": "1.1.0",
    "Microsoft.Extensions.Logging.Debug": "1.1.0",
    "Microsoft.Extensions.PlatformAbstractions": "1.1.0",
    "DasMulli.Win32.ServiceUtils": "1.0.1",
    "Microsoft.AspNetCore.StaticFiles": "1.0.0-rc2-final"
  },
  "frameworks": {
    "netcoreapp1.1": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.1.0"
        }
      },
      "imports": "dnxcore50"
    }
  },
  "runtimes": {
     "win10-x64": {},
     "centos.7-x64": {},
     "ubuntu.16.04-x64": {}
   }
}
